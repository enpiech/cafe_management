<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="fit.tdc.edu.vn.cafemanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".ui.unit_create.UnitCreateActivity"></activity>
        <activity android:name=".ui.unit_list.UnitListActivity"></activity>
        <activity android:name=".ui.category_list.CategoryListFragment">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.category_view.CategoryViewActivity"></activity>
        <activity
            android:name=".BartenderDrawerNavigation"
            android:label="@string/title_activity_bartender_drawer_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".WaiterDrawerNavigation"
            android:label="@string/title_activity_waiter_drawer_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HeadDrawerNavigation"
            android:label="@string/title_activity_head_drawer_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ManagerDrawerNavigation"
            android:label="@string/title_activity_manager_drawer_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login">

        </activity>
    </application>

</manifest>