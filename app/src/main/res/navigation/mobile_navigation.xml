<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/chefListFragment">
    <fragment
        android:id="@+id/categoryListFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.category.CategoryListFragment"
        android:label="@string/title_category_view"
        tools:layout="@layout/fragment_list" >
        <action
            android:id="@+id/categoryViewAction"
            app:destination="@id/categoryViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_categoryListFragment"
            app:destination="@id/categoryListFragment" />
        <action
            android:id="@+id/action_loginFragment_to_unitListFragment"
            app:destination="@id/unitListFragment" />
        <action
            android:id="@+id/action_loginFragment_to_zoneListFragment"
            app:destination="@id/zoneListFragment" />
        <action
            android:id="@+id/action_loginFragment_to_userListFragment"
            app:destination="@id/userListFragment" />
        <action
            android:id="@+id/action_loginFragment_to_tableListWaiterFragment"
            app:destination="@id/tableListWaiterFragment" />
        <action
            android:id="@+id/action_loginFragment_to_tableListFragment"
            app:destination="@id/tableListFragment" />
        <action
            android:id="@+id/action_loginFragment_to_storeListFragment"
            app:destination="@id/storeListFragment" />
        <action
            android:id="@+id/action_loginFragment_to_materialListFragment"
            app:destination="@id/materialListFragment" />
        <action
            android:id="@+id/action_loginFragment_to_chefListFragment"
            app:destination="@id/chefListFragment" />
        <action
            android:id="@+id/action_loginFragment_to_wareHouseListFragment"
            app:destination="@id/wareHouseListFragment" />
    </fragment>
    <fragment
        android:id="@+id/unitListFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.unit.UnitListFragment"
        android:label="@string/title_unit_view">
        <action
            android:id="@+id/unitViewAction"
            app:destination="@id/unitViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/zoneListFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.zone.ZoneListFragment"
        android:label="@string/title_zone_view" >
        <action
            android:id="@+id/zoneViewAction"
            app:destination="@id/zoneViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/zoneViewFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.zone.ZoneViewFragment"
        android:label="{title}">
        <argument
            android:name="zoneId"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
    <argument android:defaultValue='""'
        android:name="zoneId" />
    <fragment
        android:id="@+id/unitViewFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.unit.UnitViewFragment"
        android:label="{title}">
        <argument
            android:name="unitId"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/categoryViewFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.category.CategoryViewFragment"
        android:label="{title}">
        <argument
            android:name="categoryId"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/tableListFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.table.TableListFragment"
        android:label="@string/title_table_view">
        <action
            android:id="@+id/tableViewAction"
            app:destination="@id/tableViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/tableViewFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.table.TableViewFragment"
        android:label="{title}" >
        <argument
            android:name="title"
            app:argType="string" />
        <argument
            android:name="tableId"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/userListFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.user.UserListFragment"
        android:label="@string/title_user_view" >
        <action
            android:id="@+id/userViewAction"
            app:destination="@id/userViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/userViewFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.user.UserViewFragment"
        android:label="{title}">
        <argument
            android:name="userId"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/tableListWaiterFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.table_waiter.TableWaiterListFragment"
        android:label="@string/title_table_waiter_view">
        <action
            android:id="@+id/viewCurrentOrderAction"
            app:destination="@id/orderDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/storeListFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.store.StoreListFragment"
        android:label="@string/title_store_view">
        <action
            android:id="@+id/storeViewAction"
            app:destination="@id/storeDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/storeDetailFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.store.StoreDetailFragment"
        android:label="{title}">
        <argument
            android:name="storeId"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/orderListFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.order.OrderListFragment"
        android:label="{title}">
        <argument
            android:name="tableId"
            app:argType="string" />
        <argument
            android:name="title"
            app:argType="string" />
        <action
            android:id="@+id/cancelOrderAction"
            app:destination="@id/tableListWaiterFragment"
            app:launchSingleTop="true"
            app:popUpTo="@+id/tableListWaiterFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/materialListFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.material_list.MaterialListFragment"
        android:label="@string/title_material_view" >
        <action
            android:id="@+id/materialViewAction"
            app:destination="@id/materialViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/materialViewFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.material_list.MaterialDetailFragment"
        android:label="{title}">
        <argument
            android:name="materialId"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/orderDetailFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.order.OrderDetailFragment"
        android:label="{title}">
        <argument
            android:name="paymentId"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/addOrderAction"
            app:destination="@id/orderListFragment" />
        <argument
            android:name="tableId"
            app:argType="string" />
        <argument
            android:name="title"
            android:defaultValue="Thanh toÃ¡n"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/chefListFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.chef.ChefListFragment"
        android:label="@string/title_chef_view" />
    <fragment
        android:id="@+id/wareHouseListFragment"
        android:name="fit.tdc.edu.vn.cafemanagement.fragment.ware_house.WareHouseListFragment"
        android:label="WareHouseListFragment" />
</navigation>